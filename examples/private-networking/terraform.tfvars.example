# Example terraform.tfvars for private networking deployment

stack_name          = "my-runs-on-private"
github_organization = "my-github-org"
license_key         = "your-license-key-here"

aws_region = "us-east-1"

# Use single NAT gateway to save costs (~$32/month vs ~$96/month for 3)
single_nat_gateway = true

tags = {
  Environment = "production"
  ManagedBy   = "terraform"
  Project     = "runs-on"
  Networking  = "private"
}
